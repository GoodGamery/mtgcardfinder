{
  "name": "mtgcardfinder",
  "version": "1.0.1",
  "description": "MTG Card Finder Robot",
  "author": "Justin Woolley <justinwoolley@gmail.com> (https://www.pseudochaos.org)",
  "main": "server",
  "scripts": {
    "start": "node server",
    "lint": "gulp lint",
    "watch": "gulp",
    "test": "mocha"
  },
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.4",
    "express": "4.15.3",
    "lodash": "4.17.4",
    "request": "2.81.0"
  },
  "devDependencies": {
    "chai": "4.1.1",
    "chai-http": "^3.0.0",
    "eslint": "^4.2.0",
    "gulp": "^3.9.1",
    "gulp-debug": "^3.1.0",
    "gulp-eslint": "^4.0.0",
    "gulp-jsonlint": "^1.2.0",
    "gulp-util": "3.0.8",
    "mocha": "3.5.0",
    "run-sequence": "^2.0.0"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 6
    },
    "env": {
      "node": true,
      "es6": true,
      "mocha": true
    },
    "rules": {
      "no-console": 0,
      "no-mixed-spaces-and-tabs": 1,
      "no-unused-expressions": 1,
      "no-unused-vars": [
        1,
        {
          "vars": "all",
          "args": "after-used"
        }
      ],
      "strict": 2,
      "semi": [
        2,
        "always"
      ]
    }
  }
}
